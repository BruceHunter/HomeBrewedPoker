<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Authors>Bruce Hunter Jr.</Authors>
    <Company></Company>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Product>HomeBrewedPoker</Product>
    <PackageProjectUrl></PackageProjectUrl>    
    <Copyright>Copyright (c) Bruce Hunter Jr.. All rights reserved.</Copyright>
    <NeutralLanguage>en-US</NeutralLanguage>
        <Description>Free, open source Poker framework.</Description>
    <RepositoryUrl>https://github.com/BruceHunter/HomeBrewedPoker</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Version>1.0.0.0</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
